{
  "$schema": "https://raw.githubusercontent.com/LuaLS/vscode-lua/master/setting/schema.json",
  "runtime.version": "LuaJIT",
  "diagnostics.disable": [
    "luadoc-miss-module-name"
  ],
  "diagnostics.globals": [
    "love",
    "jit"
  ],
  "hint.enable": true,
  "workspace.checkThirdParty": true,
  "workspace.ignoreDir": [
    ".vscode",
    "builds",
    "resources",
    "tools"
  ],
  "workspace.library": [
    "${3rd}/love2d/library",
    "game"
  ]
}
